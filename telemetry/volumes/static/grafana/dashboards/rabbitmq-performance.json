{
  "uid": "rabbitmq_performance",
  "title": "RabbitMQ Performance",
  "schemaVersion": 38,
  "refresh": "5s",
  "time": { "from": "now-15m", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "Messages (Ready)",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_ready",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msg",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 15,
            "showPoints": "never"
          }
        }
      },
      "options": { "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "Messages (Unacked)",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_unacked",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msg",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 15,
            "showPoints": "never"
          }
        }
      },
      "options": { "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "Messages (Total)",
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "rabbitmq_queue_messages",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msg",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 15,
            "showPoints": "never"
          }
        }
      },
      "options": { "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "Throughput (msg/s)",
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_global_messages_delivered_total[10s]))",
          "legendFormat": "delivered",
          "refId": "A"
        },
        {
          "expr": "sum(rate(rabbitmq_global_messages_received_total[10s]))",
          "legendFormat": "received",
          "refId": "B"
        },
        {
          "expr": "sum(rate(rabbitmq_global_messages_acknowledged_total[10s]))",
          "legendFormat": "acknowledged",
          "refId": "C"
        },
        {
          "expr": "sum(rate(rabbitmq_global_messages_dead_lettered_rejected_total{queue_type=\"rabbit_classic_queue\",dead_letter_strategy=\"at_most_once\"}[10s]))",
          "legendFormat": "rejected",
          "refId": "D"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "msg/s" } },
      "options": { "tooltip": { "mode": "multi" } }
    },
    {
      "type": "stat",
      "title": "RabbitMQ Process (Up/Down)",
      "gridPos": { "x": 0, "y": 16, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "up{job=\"rabbitmq\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bool",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Down" },
                "1": { "text": "Up" }
              }
            }
          ]
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Connections",
      "gridPos": { "x": 8, "y": 16, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "rabbitmq_connections",
          "legendFormat": "connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "noValue": "0",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 15,
            "showPoints": "never"
          }
        }
      },
      "options": { "legend": { "showLegend": false }, "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "Channels",
      "gridPos": { "x": 16, "y": 16, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "rabbitmq_channels",
          "legendFormat": "channels",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "noValue": "0",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 15,
            "showPoints": "never"
          }
        }
      },
      "options": { "legend": { "showLegend": false }, "tooltip": { "mode": "multi" } }
    }
  ]
}
